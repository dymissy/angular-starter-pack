{
  "name": "angular-starter-pack",
  "version": "1.0.0",
  "description": "angular-starter-pack",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build:prod": "npm run clean && ./node_modules/.bin/webpack --env=prod --progress --profile --colors",
    "build:dev": "npm run clean && ./node_modules/.bin/webpack --env=dev --progress --profile --colors",
    "serve": "./node_modules/.bin/webpack-dev-server",
    "watch": "npm run build:dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dymissy/angular-starter-pack.git"
  },
  "author": "ideato srl",
  "license": "MIT",
  "dependencies": {
    "angular": "^1.6.3"
  },
  "devDependencies": {
    "copy-webpack-plugin": "git+https://github.com/dymissy/copy-webpack-plugin.git#6d271ca",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.2",
    "postcss-loader": "^1.3.3",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.2"
  }
}
